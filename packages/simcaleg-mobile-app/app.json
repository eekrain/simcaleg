{
  "expo": {
    "name": "simcaleg-mobile-app",
    "slug": "simcaleg-mobile-app",
    "scheme": "simcalegmobileapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eekrain.simcalegmobileapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.eekrain.simcalegmobileapp"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["react-native-document-scanner-plugin", "expo-router"],
    "extra": {
      "eas": {
        "projectId": "49be6440-75be-4dc6-bf2a-fca1d912fd5b"
      }
    }
  }
}
